{
  "name": "rag-hands-on-garage",
  "version": "1.2.0",
  "description": "A hands-on training project for building a Retrieval-Augmented Generation (RAG) system from scratch using Node.js and TypeScript",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "rag-garage": "./dist/cli/rag-garage.js"
  },
  "scripts": {
    "build": "tsc",
    "rag-garage": "NODE_OPTIONS='--loader ts-node/esm' ts-node --esm src/cli/rag-garage.ts",
    "dev": "NODE_OPTIONS='--loader ts-node/esm' ts-node --esm src/cli/chat.ts",
    "generate-embeddings": "NODE_OPTIONS='--loader ts-node/esm' ts-node --esm src/cli/generate-embeddings.ts",
    "chat": "NODE_OPTIONS='--loader ts-node/esm' ts-node --esm src/cli/chat.ts",
    "check-connections": "NODE_OPTIONS='--loader ts-node/esm' ts-node --esm src/cli/check-connections.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "keywords": [
    "rag",
    "retrieval-augmented-generation",
    "llm",
    "embeddings",
    "vector-search",
    "openai",
    "gemini",
    "typescript",
    "training"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "dotenv": "^16.4.7",
    "https-proxy-agent": "^7.0.6",
    "pdf-parse": "^1.1.1",
    "tunnel": "^0.0.6"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.5",
    "@types/pdf-parse": "^1.1.4",
    "@types/tunnel": "^0.0.7",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
